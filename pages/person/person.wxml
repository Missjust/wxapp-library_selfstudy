<view class="weui-cells">
  <navigator url="./children/personal-information" class="weui-cell weui-cell_access" hover-class="weui-cell_active">
    <view class="weui-cell__hd">
      <view class="user-info__avatar">
        <open-data type="userAvatarUrl"></open-data>
      </view>
    </view>
    <view class="weui-cell__bd">
      <view class="user-info__name">
        <open-data type="userNickName"></open-data>
      </view>
      <view class="font-gray-28">查看 / 修改个人信息</view>
    </view>
    <view class="weui-cell__ft weui-cell__ft_in-access"></view>
  </navigator>
</view>

<view>信用分:{{score}}</view>



<view class="weui-cell__bd">
  <navigator url="../accusehistory/accusehistory" class="weui-cell weui-cell_access" hover-class="weui-cell_active">
    <view class="weui-cell__hd">
      <image src="" class="icon"></image>
    </view>
    <view class="bun1">历史举报信息</view>
  </navigator>
</view>
<view class="weui-cell__bd">
  <navigator url="../scorehistory/scorehistory" class="weui-cell weui-cell_access" hover-class="weui-cell_active">
    <view class="weui-cell__hd">
      <image src="" class="icon"></image>
    </view>
    <view class="bun1">历史扣分信息</view>
  </navigator>
</view>


  
<view class="weui-cells">
  <view class="weui-cell weui-cell_access" hover-class="weui-cell_active">
    <view class="weui-cell__hd">
      <image src="/images/icon_logout.png" class="icon"></image>
    </view>
    <view class="bun1" bindtap="onLogout">退出登录</view>
    <view class="weui-cell__ft weui-cell__ft_in-access"></view>
  </view>
</view>