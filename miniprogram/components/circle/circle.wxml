<!--components/circle/circle.wxml-->
 <view  wx:if="{{degree=='a'}}" class="right" >
 <view class="textPlace"> {{obvious}} </view>
 </view>
 <view  wx:elif="{{degree=='b'}}" class="mid">
 <view class="textPlace"> {{obvious}} </view>
 </view>
 <view  wx:else="{{degree=='a'}}" class="left">
 <view class="textPlace"> {{obvious}} </view>
 </view>
